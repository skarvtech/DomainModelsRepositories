ParameterSetRepository CommLocalizationObjects {
	ParameterSet LocalizationParameter {
		
		Trigger INITIALPOSE {
			x: Int32
			y: Int32
			a: Double
		}
		
		Trigger GLOBALLOCALIZATION { }
		
		Trigger LOADMAP {
			filename : String
		}

		Trigger SENSORSTOUSE{
			sensorsToUse: UInt16
		}
	}
	
	ParameterSet SlamParameter {
		
		Trigger INITNEWMAP { 
			x: Int32
			y: Int32
			a: Int32 
		}
		
		Trigger SAVEMAP { 
			dirname : String
			filename : String
		}
		
	}
}